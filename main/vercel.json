{
    "version": 2,
    "builds": [
        {
            "src": "dir/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dir/main.py"
        }
    ],
    "functions": {
        "api/main.py": {
            "maxDuration": 60
        }
    }
}